@prefix : <http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#> .

<http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology> rdf:type owl:Ontology ;
                                                                        owl:versionIRI <http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology> ;
                                                                        rdfs:label "Creator: Victor Colina Coca"@en ,
                                                                                   "Ontology Name: The Wire Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasCast
:hasCast rdf:type owl:ObjectProperty ;
         owl:inverseOf :isCastOf ;
         rdf:type owl:FunctionalProperty ,
                  owl:AsymmetricProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Show ;
         rdfs:range :Cast ;
         rdfs:comment "Relates a show with its cast team"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasCastMember
:hasCastMember rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasMember ;
               owl:inverseOf :isCastMemberOf ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :Cast ;
               rdfs:range :Actor ;
               rdfs:comment "Relates a cast group with its actors/actresses"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              owl:inverseOf :isCharacterFrom ;
              rdf:type owl:InverseFunctionalProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain :Show ;
              rdfs:range :FictionalCharacter ;
              rdfs:comment "Relates a show with the fictional characters of its story"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasCrew
:hasCrew rdf:type owl:ObjectProperty ;
         owl:inverseOf :isCrewOf ;
         rdf:type owl:FunctionalProperty ,
                  owl:AsymmetricProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Show ;
         rdfs:range :Crew ;
         rdfs:comment "Relates a show with its crew team"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasCrewMember
:hasCrewMember rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasMember ;
               owl:inverseOf :isCrewMemberOf ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :Crew ;
               rdfs:range :RealPerson ;
               rdfs:comment "Relates a crew with its members"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasEpisode
:hasEpisode rdf:type owl:ObjectProperty ;
            owl:inverseOf :isEpisodeOf ;
            rdf:type owl:InverseFunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Season ;
            rdfs:range :Episode ;
            rdfs:comment "Relates a season with its episodes"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Team ;
           rdfs:range :Person ;
           rdfs:comment "Relates a team with its members"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasNextEpisode
:hasNextEpisode rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasPreviousEpisode ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain :Episode ;
                rdfs:range :Episode ;
                rdfs:comment "Relates an episode with the next episode of the season"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasNextSeason
:hasNextSeason rdf:type owl:ObjectProperty ;
               owl:inverseOf :hasPreviousSeason ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ,
                        owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :Season ;
               rdfs:range :Season ;
               rdfs:comment "Relates a season with the next season of a show"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasPreviousEpisode
:hasPreviousEpisode rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ,
                             owl:InverseFunctionalProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain :Episode ;
                    rdfs:range :Episode ;
                    rdfs:comment "Relates an episode with the previous episode of the season"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasPreviousSeason
:hasPreviousSeason rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ,
                            owl:InverseFunctionalProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :Season ;
                   rdfs:range :Season ;
                   rdfs:comment "Relates a season with the previous season of a show"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#hasSeason
:hasSeason rdf:type owl:ObjectProperty ;
           owl:inverseOf :isSeasonOf ;
           rdf:type owl:InverseFunctionalProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Show ;
           rdfs:range :Season ;
           rdfs:comment "Relates a show with its seasons"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCastMemberOf
:isCastMemberOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMemberOf ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain :Actor ;
                rdfs:range :Cast ;
                rdfs:comment "Relates an actor with the casts he or she is a part of"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCastOf
:isCastOf rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Cast ;
          rdfs:range :Show ;
          rdfs:comment "Relates the cast team with the show that belongs to"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCharacterFrom
:isCharacterFrom rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :FictionalCharacter ;
                 rdfs:range :Show ;
                 rdfs:comment "Relates a fictional character with the show that belongs to"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCountryOfProduction
:isCountryOfProduction rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isProducedIn ;
                       rdf:type owl:InverseFunctionalProperty ,
                                owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :Country ;
                       rdfs:range :Show ;
                       rdfs:comment "Relates a country with the shows produced in there"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCountryOriginOf
:isCountryOriginOf rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isFrom ;
                   rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :Country ;
                   rdfs:range :RealPerson .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCrewMemberOf
:isCrewMemberOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMemberOf ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain :RealPerson ;
                rdfs:range :Crew ;
                rdfs:comment "Relates a person who is not an actor with the teams he or she is part of"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isCrewOf
:isCrewOf rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ,
                   owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :Crew ;
          rdfs:range :Show ;
          rdfs:comment "Relates a crew team with the show that belongs to"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isEpisodeOf
:isEpisodeOf rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain :Episode ;
             rdfs:range :Season ;
             rdfs:comment "Relates an episode with the season that belongs to"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isFrom
:isFrom rdf:type owl:ObjectProperty ,
                 owl:AsymmetricProperty ,
                 owl:IrreflexiveProperty ;
        rdfs:domain :RealPerson ;
        rdfs:range :Country .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Person ;
            rdfs:range :Team ;
            rdfs:comment "Relates a person with a group that is member of"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isPlacementOf
:isPlacementOf rdf:type owl:ObjectProperty ;
               owl:inverseOf :isSetIn ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :Location ;
               rdfs:range :Show ;
               rdfs:comment "Relates a location with the shows that its story occurs there"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isPlayedBy
:isPlayedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :plays ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :FictionalCharacter ;
            rdfs:range :Actor ;
            rdfs:comment "Relates a fictional character with the actor who plays it"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isProducedIn
:isProducedIn rdf:type owl:ObjectProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain :Show ;
              rdfs:range :Country ;
              rdfs:comment "Relates the show with the country or countries involved in the production"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isSeasonOf
:isSeasonOf rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Season ;
            rdfs:range :Show ;
            rdfs:comment "Relates a Season with the show that belongs"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#isSetIn
:isSetIn rdf:type owl:ObjectProperty ,
                  owl:AsymmetricProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Show ;
         rdfs:range :Location ;
         rdfs:comment "Relates the show with the place where story occurs"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#plays
:plays rdf:type owl:ObjectProperty ,
                owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
       rdfs:domain :Actor ;
       rdfs:range :FictionalCharacter ;
       rdfs:comment "Relates an actor with the fictional characters played by him or her"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#characterDescription
:characterDescription rdf:type owl:DatatypeProperty ;
                      rdfs:domain :FictionalCharacter ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Description of a fictional character"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#cityName
:cityName rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :City ;
          rdfs:range xsd:string ;
          rdfs:comment "Name of a city"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#countryName
:countryName rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Country ;
             rdfs:range xsd:string ;
             rdfs:comment "Name of a country"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#duration
:duration rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Episode ;
          rdfs:range xsd:int ;
          rdfs:comment "Duration in minutes of an episode"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#episodeOrder
:episodeOrder rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Episode ;
              rdfs:range xsd:int ;
              rdfs:comment "Number to determine the order of season's episodes"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#episodeTitle
:episodeTitle rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Episode ;
              rdfs:range xsd:string ;
              rdfs:comment "Name to identify an episode"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#latitude
:latitude rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :City ;
          rdfs:range xsd:float ;
          rdfs:comment "Latitude of a city in degrees"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#longitude
:longitude rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :City ;
           rdfs:range xsd:float ;
           rdfs:comment "Longitude of a city in degrees"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#numberOfEpisodes
:numberOfEpisodes rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Season ;
                  rdfs:range xsd:int ;
                  rdfs:comment "Number of episodes of a season"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#numberOfSeasons
:numberOfSeasons rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Show ;
                 rdfs:range xsd:int ;
                 rdfs:comment "Number of seasons of a show"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#personName
:personName rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Person ;
            rdfs:range xsd:string ;
            rdfs:comment "Full name of person"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#seasonOrder
:seasonOrder rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Season ;
             rdfs:range xsd:int ;
             rdfs:comment "Number to determine the order of show's seasons"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#showTitle
:showTitle rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Show ;
           rdfs:range xsd:string ;
           rdfs:comment "Name to identify TV serie"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Actor
:Actor rdf:type owl:Class ;
       rdfs:subClassOf :RealPerson ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :isFrom ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Country
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :personName ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ;
       rdfs:comment "Represents a person who works as actor/actress in the show"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Cast
:Cast rdf:type owl:Class ;
      rdfs:subClassOf :Team ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCastMember ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Actor
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isCastOf ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Show
                      ] ;
      owl:disjointWith :Crew ;
      rdfs:comment "Represents the group of actors involved in a TV serie"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Location ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :cityName ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :latitude ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange [ rdf:type rdfs:Datatype ;
                                          owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                 owl:onDatatype xsd:float ;
                                                                 owl:withRestrictions ( [ xsd:minInclusive "-90.0"^^xsd:float
                                                                                        ]
                                                                                      )
                                                               ]
                                                               [ rdf:type rdfs:Datatype ;
                                                                 owl:onDatatype xsd:float ;
                                                                 owl:withRestrictions ( [ xsd:maxInclusive "90.0"^^xsd:float
                                                                                        ]
                                                                                      )
                                                               ]
                                                             )
                                        ]
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :longitude ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange [ rdf:type rdfs:Datatype ;
                                          owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                 owl:onDatatype xsd:float ;
                                                                 owl:withRestrictions ( [ xsd:minInclusive "-180.0"^^xsd:float
                                                                                        ]
                                                                                      )
                                                               ]
                                                               [ rdf:type rdfs:Datatype ;
                                                                 owl:onDatatype xsd:float ;
                                                                 owl:withRestrictions ( [ xsd:maxInclusive "180.0"^^xsd:float
                                                                                        ]
                                                                                      )
                                                               ]
                                                             )
                                        ]
                      ] ;
      rdfs:comment "Represents a city and is used to relate the TV series to the city where the story takes place."@en ;
      rdfs:sameAs "https://dbpedia.org/page/City"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Commander
:Commander rdf:type owl:Class ;
           rdfs:subClassOf :PoliceOfficer ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :characterDescription ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :personName ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ;
           rdfs:comment "Represents fictional police officers with command position"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Location ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :countryName ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] ;
         rdfs:comment "Represents a country"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Creator
:Creator rdf:type owl:Class ;
         rdfs:subClassOf :RealPerson ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :isFrom ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Country
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :personName ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Crew
:Crew rdf:type owl:Class ;
      rdfs:subClassOf :Team ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCrewMember ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Creator
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCrewMember ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Director
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCrewMember ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :ExecutiveProducer
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCrewMember ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Producer
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCrewMember ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Writer
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isCrewOf ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Show
                      ] ;
      rdfs:comment "Represents the group of people involved in a TV serie apart from actors"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Criminal
:Criminal rdf:type owl:Class ;
          rdfs:subClassOf :FictionalCharacter ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :characterDescription ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :personName ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:comment "Represents all fictional characters who live outside of the law"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Detective
:Detective rdf:type owl:Class ;
           rdfs:subClassOf :PoliceOfficer ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :characterDescription ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :personName ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ;
           rdfs:comment "Represents fictional police officers with investigator position"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Director
:Director rdf:type owl:Class ;
          rdfs:subClassOf :RealPerson ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isFrom ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Country
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :personName ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:comment "Represents a person who works as a director in the show"@en .


###  http://www.semanticweb.org/victor/ontologies/2022/3/TVSeriesOntology#Episode
:Episode rdf:type owl:Class ;
